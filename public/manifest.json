{
  "manifest_version": 2,
  "name": "Pl ITechCraft extension",
  "author": "vitt",
  "version": "0.1.0",
  "description": "!Chrome extension to manage pl api with jira(flexwisehealth)",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "declarativeContent",
    "notifications",
    "https://flexwisehealth.atlassian.net/browse/*"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'"
}
